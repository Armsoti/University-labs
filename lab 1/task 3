n = int(input("input number from 1 to 9"))
while n<=1 or n>=9:
    n = int(input("input number from 1 to 9"))

for i in range(1, n + 1):
    num = 1
    for j in range(n, 0, -1):
        if j > i:
            print(" ", end = " ")
        else:
            print(num, end = " ")
            num += 1
    print("")
