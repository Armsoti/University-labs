from mod import average_of_elements
def expression (m):
    z = (m ** 0.5)+10
    return z


m = int(input("Введіть значення m: "))
print ("Значення виразу z = ", expression(m))
x = int(input("Введіть від якого числа (х) знаходити середнє арифметичне усіх парних чисел: "))
y = int(input("Введіть до якого числа (y) знаходити середнє арифметичне усіх парних чисел: "))
while x > y:
    x = int(input("у не може бути менше х, введіть ще раз від якого числа (х) знаходити середнє арифметичне усіх парних чисел: "))
    y = int(input("Введіть до якого числа (y) знаходити середнє арифметичне усіх парних чисел: "))
print("Середнє арифметичне парних чисел від х до у = ", average_of_elements (x, y))
